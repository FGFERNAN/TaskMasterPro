ios.get(\"http://localhost:4000/api-v1/user\");\r\n\r\n      if (response.data && Array.isArray(response.data.data)) {\r\n        this.users = response.data.data.map((user) => ({\r\n          id: user.id,\r\n          nombre: user.nombre,\r\n          apellidos: user.apellidos,\r\n          telefono: user.telefono,\r\n          password: user.password,\r\n          rolID: user.rolID,\r\n          tipo_documento: user.tipo_documento,\r\n        }));\r\n        console.log(\"Usuarios cargados correctamente:\", this.users);\r\n      } else {\r\n        console.error(\"Datos recibidos no válidos:\", response.data);\r\n      }\r\n    } catch (error) {\r\n      console.error(\"Error al cargar usuarios:\", error);\r\n    }\r\n  },\r\n  methods: {\r\n    redirectToCreateUser() {\r\n      this.$router.push(\"/crear-usuario\");\r\n    },\r\n    },\r\n    async deleteUser(userId) {\r\n      if (confirm(\"¿Estás seguro que deseas eliminar este usuario?\")) {\r\n        try {\r\n          console.log(\"Usuario eliminado correctamente:\", userId);\r\n        } catch (error) {\r\n          console.error(\"Error al eliminar el usuario:\", error);\r\n        }\r\n      }\r\n    },\r\n};\r\n</script>"],"mappings":";;AAuIA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAgB,CAAC,EAChC;QAAED,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAiB,CAAC,EACjC;QAAED,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAmB,CAAC,EACnC;QAAED,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAc,CAAC;IAElC,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACFC,OAAO,CAACC,GAAG,CAAC,kCAAkC,CAAC;MAC/C,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,mCAAmC,CAAC;MAErE,IAAID,QAAQ,CAACR,IAAG,IAAKU,KAAK,CAACC,OAAO,CAACH,QAAQ,CAACR,IAAI,CAACA,IAAI,CAAC,EAAE;QACtD,IAAI,CAACC,KAAI,GAAIO,QAAQ,CAACR,IAAI,CAACA,IAAI,CAACY,GAAG,CAAEC,IAAI,KAAM;UAC7CV,EAAE,EAAEU,IAAI,CAACV,EAAE;UACXW,MAAM,EAAED,IAAI,CAACC,MAAM;UACnBC,SAAS,EAAEF,IAAI,CAACE,SAAS;UACzBC,QAAQ,EAAEH,IAAI,CAAC