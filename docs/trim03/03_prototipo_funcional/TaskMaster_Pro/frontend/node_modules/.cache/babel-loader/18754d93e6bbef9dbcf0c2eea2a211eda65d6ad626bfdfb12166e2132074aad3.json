{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      users: [],\n      roles: [{\n        id: 1,\n        name: \"Administrador\"\n      }, {\n        id: 2,\n        name: \"Líder Proyecto\"\n      }, {\n        id: 3,\n        name: \"Miembro Proyecto\"\n      }, {\n        id: 4,\n        name: \"StakeHolder\"\n      }]\n    };\n  },\n  async created() {\n    try {\n      console.log(\"Realizando solicitud GET a /user\");\n      const response = await axios.get(\"http://localhost:4000/api-v1/user\");\n      if (response.data && Array.isArray(response.data.data)) {\n        this.users = response.data.data.map(user => ({\n          id: user.id,\n          nombre: user.nombre,\n          apellidos: user.apellidos,\n          telefono: user.telefono,\n          password: user.password,\n          rolID: user.rolID,\n          tipo_documento: user.tipo_documento\n        }));\n        console.log(\"Usuarios cargados correctamente:\", this.users);\n      } else {\n        console.error(\"Datos recibidos no válidos:\", response.data);\n      }\n    } catch (error) {\n      console.error(\"Error al cargar usuarios:\", error);\n    }\n  },\n  methods: {\n    redirectToCreateUser() {\n      this.$router.push(\"/crear-usuario\");\n    }\n  },\n  async deleteUser(userId) {\n    if (confirm(\"¿Estás seguro que deseas eliminar este usuario?\")) {\n      try {\n        console.log(\"Usuario eliminado correctamente:\", userId);\n      } catch (error) {\n        console.error(\"Error al eliminar el usuario:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","users","roles","id","name","created","console","log","response","get","Array","isArray","map","user","nombre","apellidos","telefono","password","rolID","tipo_documento","error","methods","redirectToCreateUser","$router","push","deleteUser","userId","confi