    <div class=\"bg-custom\">\r\n      <!-- Navbar para móvil -->\r\n      <nav class=\"navbar navbar-expand-lg navbar-light bg-light d-lg-none\">\r\n        <div class=\"container-fluid\">\r\n          <button class=\"navbar-toggler\" type=\"button\" @click=\"toggleMenu\">\r\n            <span class=\"navbar-toggler-icon\"></span>\r\n          </button>\r\n          <a class=\"navbar-brand\" href=\"#\">Menú</a>\r\n        </div>\r\n      </nav>\r\n  \r\n      <!-- Menú lateral (Offcanvas para móviles) -->\r\n      <div class=\"offcanvas offcanvas-start\" :class=\"{ show: menuOpen }\" tabindex=\"-1\">\r\n        <div class=\"offcanvas-header\">\r\n          <h5 class=\"offcanvas-title\">Menú</h5>\r\n          <button type=\"button\" class=\"btn-close\" @click=\"toggleMenu\"></button>\r\n        </div>\r\n        <div class=\"offcanvas-body\">\r\n          <ul class=\"nav flex-column\">\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link mi-link\" to=\"/creacionProyecto\">\r\n                <i class=\"fa-solid fa-folder-plus me-1\"></i> Crear Proyecto\r\n              </router-link>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link mi-link\" to=\"/plantillasProyecto\">\r\n                <i class=\"fa-solid fa-folder-tree me-1\"></i> Utilizar Plantillas\r\n              </router-link>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link mi-link\" to=\"/misTareas\">\r\n                <i class=\"fa-solid fa-list-check me-1\"></i> Mis Tareas\r\n              </router-link>\r\n            </li>\r\n            <li class=\"nav-item dropdown custom-dropdown\">\r\n              <a class=\"nav-link dropdown-toggle mi-link\" href=\"#\" @click=\"toggleProyectos\">\r\n                <i class=\"fa-solid fa-folder-minus me-1\"></i> Proyectos\r\n              </a>\r\n              <ul class=\"dropdown-menu\" v-if=\"proyectosOpen\">\r\n 