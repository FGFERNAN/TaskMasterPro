{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport api from \"@/services/api\";\nexport default {\n  data() {\n    return {\n      email: '',\n      password: '',\n      showPassword: false,\n      errors: {}\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      const form = this.$refs.form;\n\n      // Desactiva validación automática del navegador y aplica validación manual con Bootstrap\n      if (!form.checkValidity()) {\n        form.classList.add('was-validated');\n        return;\n      }\n      this.errors = {};\n      try {\n        const response = await api.post(\"/login\", {\n          email: this.email,\n          password: this.password\n        });\n        console.log(response.data);\n        alert('Inicio de Sesion Exitoso');\n        this.$router.push(\"/perfil-completo\");\n      } catch (error) {\n        if (error.response && error.response.data) {\n          const serverErrors = error.response.data;\n          if (serverErrors.message === 'Password Error') {\n            this.errors.password = 'Contraseña incorrecta.';\n          } else if (serverErrors.message === 'The user not created') {\n            this.errors.email = 'El correo no está registrado.';\n          }\n        }\n      }\n    },\n    togglePasswordVisibility() {\n      const passwordField = document.getElementById('password');\n      passwordField.type = this.showPassword ? 'text' : 'password';\n    },\n    redirectToRecoverPassword() {\n      // Redirección a la vista de Recuperar Contraseña\n      this.$router.push('/recuperar-contraseña');\n    }\n  }\n};","map":{"version":3,"names":["api","data","email","password","showPassword","errors","methods","handleSubmit","form","$refs","checkValidity","classList","add","response","post","console","log","alert","$router","push","error","serverErrors","message","togglePasswordVisibility","passwordField","document","getElementById","type","redirectToRecoverPassword"],"sources":["C:\\xampp\\htdocs\\TaskMasterPro\\trim03\\04_prototipo_funcional\\TaskMaster_Pro\\frontend\\src\\components\\IniciarSesion.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row justify-content-center\">\r\n      <div class=\"col-md-6\">\r\n        <div class=\"text-center\">\r\n          <img src=\"../assets/img/logotipo.png\" class=\"img-fluid mx-auto d-block logo1\" width=\"400\" alt=\"Logo\" />\r\n        </div>\r\n        <form ref=\"form\" @submit.prevent=\"handleSubmit\" class=\"mx-auto needs-validation custom-iniciarSesion\" novalidate>\r\n          <div class=\"mb-3\">\r\n            <label for=\"email\" class=\"form-label\">Correo Electrónico</label>\r\n            <div class=\"input-group mb-3 has-validation\">\r\n              <span class=\"input-group-text\"><i class=\"fa-regular fa-envelope\"></i></span>\r\n              <div class=\"form-floating\">\r\n                <input v-model=\"email\" type=\"email\" class=\"form-control border-input\"\r\n                  :class=\"{ 'is-invalid': errors.email }\" id=\"email\" placeholder=\"Correo electrónico\" required />\r\n                <label for=\"email\">Correo Electrónico</label>\r\n                <div class=\"invalid-feedback\">\r\n                  {{ errors.email || \"Correo Invalido (Por favor ingresar el correo en el siguiente formato 'xxxx@xxx.xxx').\" }}\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"mb-3 mt-4\">\r\n            <label for=\"password\" class=\"form-label\">Contraseña</label>\r\n            <div class=\"input-group mb-3 has-validation\">\r\n              <span class=\"input-group-text\"><i class=\"fa-solid fa-lock\"></i></span>\r\n              <div class=\"form-floating\">\r\n                <input v-model=\"password\" type=\"password\" class=\"form-control border-input\"\r\n                  :class=\"{ 'is-invalid': errors.password }\" id=\"password\" placeholder=\"Contraseña\" maxlength=\"20\"\r\n                  minlength=\"8\" required />\r\n                <label for=\"password\">Contraseña</label>\r\n                <div class=\"invalid-feedback\">\r\n                  {{ errors.password || \"Contraseña Invalida, asegurate de que cumpla los requisitos minimos (tener minimo 8 caracteres, una letra mayuscula, una letra minuscula y un caracter especial (#,$,*).)\" }}\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"form-check form-switch ver-contraseña\">\r\n              <input v-model=\"showPassword\" class=\"form-check-input\" type=\"checkbox\" role=\"switch\" id=\"viewPassword\"\r\n                @change=\"togglePasswordVisibility\" />\r\n              <label class=\"form-check-label\" for=\"viewPassword\">Ver Contraseña</label>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"d-grid gap-2 col-6 mx-auto\">\r\n            <button class=\"btn btn-primary mt-3 buton-iniciar-sesion\" type=\"submit\">\r\n              Iniciar Sesión\r\n            </button>\r\n            <button type=\"button\" class=\"btn btn-primary mt-2 mb-4 buton-iniciar-sesion\"\r\n              @click=\"redirectToRecoverPassword\">\r\n              Recuperar Contraseña\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport api from \"@/services/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      showPassword: false,\r\n      errors: {},\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSubmit() {\r\n      const form = this.$refs.form;\r\n\r\n      // Desactiva validación automática del navegador y aplica validación manual con Bootstrap\r\n      if (!form.checkValidity()) {\r\n        form.classList.add('was-validated');\r\n        return;\r\n      }\r\n      this.errors = {};\r\n      try {\r\n        const response = await api.post(\"/login\", {\r\n          email: this.email,\r\n          password: this.password,\r\n        });\r\n        console.log(response.data);\r\n        alert('Inicio de Sesion Exitoso');\r\n        this.$router.push(\"/perfil-completo\");\r\n      } catch (error) {\r\n        if (error.response && error.response.data) {\r\n          const serverErrors = error.response.data;\r\n          if (serverErrors.message === 'Password Error') {\r\n            this.errors.password = 'Contraseña incorrecta.';\r\n          } else if (serverErrors.message === 'The user not created') {\r\n            this.errors.email = 'El correo no está registrado.';\r\n          }\r\n        }\r\n      }\r\n    },\r\n    togglePasswordVisibility() {\r\n      const passwordField = document.getElementById('password');\r\n      passwordField.type = this.showPassword ? 'text' : 'password';\r\n    },\r\n    redirectToRecoverPassword() {\r\n      // Redirección a la vista de Recuperar Contraseña\r\n      this.$router.push('/recuperar-contraseña');\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Personaliza estilos aquí */\r\n.bg-custom {\r\n  background-color: #d31e1e;\r\n  /* Ajusta según tu diseño */\r\n}\r\n</style>\r\n"],"mappings":";AA8DA,OAAOA,GAAE,MAAO,gBAAgB;AAEhC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,KAAK;MACnBC,MAAM,EAAE,CAAC;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,IAAG,GAAI,IAAI,CAACC,KAAK,CAACD,IAAI;;MAE5B;MACA,IAAI,CAACA,IAAI,CAACE,aAAa,CAAC,CAAC,EAAE;QACzBF,IAAI,CAACG,SAAS,CAACC,GAAG,CAAC,eAAe,CAAC;QACnC;MACF;MACA,IAAI,CAACP,MAAK,GAAI,CAAC,CAAC;MAChB,IAAI;QACF,MAAMQ,QAAO,GAAI,MAAMb,GAAG,CAACc,IAAI,CAAC,QAAQ,EAAE;UACxCZ,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACFY,OAAO,CAACC,GAAG,CAACH,QAAQ,CAACZ,IAAI,CAAC;QAC1BgB,KAAK,CAAC,0BAA0B,CAAC;QACjC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,kBAAkB,CAAC;MACvC,EAAE,OAAOC,KAAK,EAAE;QACd,IAAIA,KAAK,CAACP,QAAO,IAAKO,KAAK,CAACP,QAAQ,CAACZ,